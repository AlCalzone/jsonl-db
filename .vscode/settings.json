{
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"eslint.enable": true,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": true
		}
	},
	"editor.rulers": [
		72,
		80,
		120
	],
	"vitest.commandLine": "npm run test:ts --",
	"vitest.enable": true
}
